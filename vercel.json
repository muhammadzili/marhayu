{
  "rewrites": [
    {
      "source": "/auth",
      "destination": "/auth.html",
      "has": [
        {
          "type": "query",
          "key": "config",
          "value": "true"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/api/db.js",
      "destination": "/",
      "permanent": false
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}

